{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["last 50 versions"],
          "electron": 1.6,
          "node": "current",
        },
        "modules": false,
        "useBuiltIns": true,
        "debug": false
      }
    ],
    "stage-0",
    "react",
    "react-native",
  ],
  "plugins": [
    [
      "import",{
        "libraryName": "antd",
        "style": true
      }
    ],
    "react-hot-loader/babel"
  ]
}
